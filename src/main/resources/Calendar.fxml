<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Controller">
   <children>
      <Button fx:id="addButton" layoutX="25.0" layoutY="28.0" mnemonicParsing="false" onAction="#addCalendar" prefWidth="55.0" text="ADD" textFill="#0a0a0a" />
      <Button fx:id="editButton" layoutX="97.0" layoutY="28.0" mnemonicParsing="false" onAction="#editCalendar" prefWidth="55.0" text="EDIT" />
      <Button fx:id="deleteButton" layoutX="173.0" layoutY="28.0" mnemonicParsing="false" onAction="#deleteCalendar" prefWidth="55.0" text="DELETE" />
      <ScrollPane hbarPolicy="NEVER" layoutX="25.0" layoutY="70.0" prefHeight="197.0" prefWidth="352.0">
         <content>
            <ListView fx:id="listCalendar" prefHeight="195.0" prefWidth="350.0" />
         </content>
      </ScrollPane>
      <ScrollPane fx:id="detailScrollPane" hbarPolicy="NEVER" layoutX="25.0" layoutY="300.0">
         <content>
            <TextArea fx:id="detailCalendar" editable="false" prefHeight="273.0" prefWidth="350.0" wrapText="true" />
         </content>
      </ScrollPane>
      <Button layoutX="322.0" layoutY="28.0" mnemonicParsing="false" onAction="#exitCalendar" prefHeight="25.0" prefWidth="55.0" text="EXIT" />
      <Button layoutX="245.0" layoutY="28.0" mnemonicParsing="false" onAction="#searchCalendar" text="SEARCH" />
   </children>
</AnchorPane>
