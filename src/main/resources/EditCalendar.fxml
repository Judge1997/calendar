<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.EditController">
    <children>
        <DatePicker fx:id="dateEditCalendar" layoutX="87.0" layoutY="31.0" prefHeight="25.0" prefWidth="121.0" />
        <Label layoutX="25.0" layoutY="30.0" prefHeight="27.0" prefWidth="52.0" text="Date :">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label layoutX="25.0" layoutY="100.0" prefHeight="37.0" prefWidth="52.0" text="Title :">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextArea fx:id="titleEditCalendar" layoutX="87.0" layoutY="106.0" prefHeight="0.0" prefWidth="262.0" />
        <Label layoutX="25.0" layoutY="133.0" prefHeight="44.0" prefWidth="63.0" text="Detail :">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <ScrollPane hbarPolicy="NEVER" layoutX="25.0" layoutY="177.0">
            <content>
                <TextArea fx:id="detailEditCalendar" prefHeight="322.0" prefWidth="350.0" wrapText="true" />
            </content>
        </ScrollPane>
        <Button fx:id="doneButton" layoutX="165.0" layoutY="527.0" mnemonicParsing="false" onAction="#doneEditCalendar" prefHeight="44.0" prefWidth="73.0" text="Done" />
        <ComboBox fx:id="hourEditCalendar" layoutX="145.0" layoutY="70.0" prefHeight="25.0" prefWidth="63.0" />
        <Label layoutX="25.0" layoutY="69.0" text="Time :">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label layoutX="87.0" layoutY="69.0" text="Hour :">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label layoutX="211.0" layoutY="69.0" text="Minute :">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <ComboBox fx:id="minuteEditCalendar" layoutX="286.0" layoutY="70.0" prefHeight="25.0" prefWidth="63.0" />
      <Button mnemonicParsing="false" onAction="#toMainWindow" text="BACK" />
      <Label layoutX="218.0" layoutY="30.0" text="Status :">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="statusEditCalendar" layoutX="286.0" layoutY="31.0" prefHeight="25.0" prefWidth="89.0" />
    </children>
</AnchorPane>
